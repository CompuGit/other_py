{

    ".json": {
        "f_re": "^[a-zA-Z][0-9.]*-[0-9.]*[json]*$",
        "d_re" : "(^[\\{\\[\\s\\]\\}]*)|(\\\"[\\w_.-]*\\\":\\s\\\"[\\w_.-]*\\\",*)|(\\\"[\\w_.-]*\\\":[\\s0-9]*,*)"
    },
    ".csv": {
        "f_re": "^[a-zA-Z ]*[0-9_]*\\W[csv]*$",
        "d_re" : "^[\\w, ]*[\\w]"
    },
    ".xml": {
        "f_re": "^[a-zA-Z][0-9.]*\\W[0-9]*-[0-9.]*\\W[0-9]*_[a-zA-Z0-9]*\\W[xml]*$",
        "d_re" : "^<\\?xml.*\\?>|^<[\\w\\W ]*"
    }

}
